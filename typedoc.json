{
  "entryPoints": ["src"],
  "tsconfig": "tsconfig.json",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "plugin": ["typedoc-plugin-markdown"], 
  "hideGenerator": true,
  "readme": "none",
  "out": "docs"          // HTML if no markdown plugin; Markdown if plugin is present
}
